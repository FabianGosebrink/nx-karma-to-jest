{
  "name": "@offeringsolutions/nx-karma-to-jest",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/FabianGosebrink/nx-karma-to-jest.git"
  },
  "description": "Replacing Karma with Jest in an nx workspace",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "test": "npm run build && jasmine src/**/*_spec.js",
    "release:dryrun": "release-it --dry-run",
    "release": "release-it"
  },
  "keywords": [
    "schematics"
  ],
  "author": "Fabian Gosebrink",
  "license": "MIT",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.0.5",
    "@angular-devkit/schematics": "^9.0.5",
    "typescript": "~3.7.4"
  },
  "devDependencies": {
    "@schematics/angular": "^9.0.5",
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0",
    "release-it": "^13.0.0",
    "rxjs": "^6.5.4"
  }
}
